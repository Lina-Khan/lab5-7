    "redux-thunk": "^2.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "react-redux": "^5.0.6",
    "react-router-redux": "^4.0.8",
    "antd" : "^2.13.11"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.16.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "jest" : "^21.2.1"
